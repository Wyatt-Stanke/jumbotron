{
	"name": "jumbotron",
	"version": "0.0.1",
	"private": true,
	"workspaces": [
		"app",
		"parser",
		"modkit",
		"typings-core",
		"injector/*"
	],
	"devDependencies": {
		"@biomejs/biome": "2.3.9",
		"esbuild": "0.27.2",
		"javascript-obfuscator": "5.1.0",
		"tsx": "4.21.0",
		"typescript": "5.9.3"
	},
	"type": "module",
	"scripts": {
		"build": "npm run build --workspaces --if-present",
		"test": "npm run test --workspaces --if-present",
		"format": "biome format --write --vcs-enabled=true --vcs-use-ignore-file=true --vcs-client-kind=git .",
		"format:check": "biome format --vcs-enabled=true --vcs-use-ignore-file=true --vcs-client-kind=git .",
		"lint": "biome lint --write --vcs-enabled=true --vcs-use-ignore-file=true --vcs-client-kind=git .",
		"lint:check": "biome lint --vcs-enabled=true --vcs-use-ignore-file=true --vcs-client-kind=git .",
		"check": "biome check --write --vcs-enabled=true --vcs-use-ignore-file=true --vcs-client-kind=git .",
		"check:ci": "biome ci --vcs-enabled=true --vcs-use-ignore-file=true --vcs-client-kind=git ."
	}
}
